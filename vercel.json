{
  "version": 2,
  "installCommand": "npm install --production=false",
  "buildCommand": "npx nest build",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 30,
      "includeFiles": "dist/**"
    }
  }
}
