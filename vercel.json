{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api-json",
      "dest": "/api/swagger-json"
    },
    {
      "src": "/swagger",
      "dest": "/public/swagger.html"
    },
    {
      "src": "/(.*)",
      "dest": "/api"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    },
    "api/swagger-json.js": {
      "maxDuration": 10
    }
  }
}
